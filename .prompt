# get current branch in git repo
function parse_git_branch() {
    BRANCH=`git branch 2> /dev/null | sed -e '/^[^*]/d' -e 's/* \(.*\)/\1/'`
    if [ ! "${BRANCH}" == "" ]
    then
        echo "[${BRANCH}]"
    else
        echo ""
    fi
}

export PS1="\e[38;5;254m\`parse_git_branch\`\e[38;5;245m\w\e[1m\e[38;5;208m\\$ \e[m"
